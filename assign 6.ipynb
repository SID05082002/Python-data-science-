    "list1 = [\"M\", \"na\", \"i\", \"Ke\"]\n",
    "list2 = [\"y\", \"me\", \"s\", \"lly\"]\n",
    "\n",
    "max_len = max(len(list1), len(list2))\n",
    "\n",
    "result = []\n",
    "\n",
    "\n",
    "for i in range(max_len):\n",
    "    if i < len(list1):\n",
    "        result.append(list1[i])\n",
    "    if i < len(list2):\n",
    "        result.append(list2[i])\n",
    "\n",
    "print(result)\n"
