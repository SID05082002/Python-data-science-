    "from datetime import datetime\n",
    "\n",
    "\n",
    "list1 = [1991, 1992, 1994, 1996, 1998, 2000, 2001, 2002, 2004]\n",
    "\n",
    "\n",
    "current_year = datetime.now().year\n",
    "\n",
    "age_list = list(map(lambda x: current_year - x, list1))\n",
    "\n",
    "print(\"Age list:\", age_list)\n"
