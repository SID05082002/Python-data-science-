   "source": [
    "n = int(input(\"Enter the number of terms: \"))\n",
    "\n",
    "term = 0\n",
    "sum_series = 0\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    term = term * 10 + 2\n",
    "    sum_series += term\n",
    "\n",
    "print(f\"The sum of the series up to {n} terms is: {sum_series}\")\n"
